<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Icon="http://metro.mahapps.com/winfx/xaml/iconpacks">

    <!-- 基本カラー -->
    <Color x:Key="LeftWindowColor1">#202020</Color>
    <Color x:Key="LeftWindowColor12">#FF7F3F</Color>
    <Color x:Key="LeftWindowColor2">#202050</Color>
    <Color x:Key="LeftWindowColor22">#A02020</Color>
    <Color x:Key="AccentColor">#7FFFD4</Color>
    <Color x:Key="ButtonThemeColor">#6A5ACD</Color>
    <Color x:Key="ButtonThemeColor2">#4A5ACD</Color>
    <Color x:Key="BracketColor">#ffc0a8</Color>
    <Color x:Key="CheckToggleColor1">#202020</Color>
    <Color x:Key="CheckToggleColor2">#A0A0A0</Color>
    <Color x:Key="TextColor">#FFFFFF</Color>
    <Color x:Key="TagColor">#FFC0A8</Color>
    <Color x:Key="RippleColor">#7FFFD4</Color>

    <Color x:Key="TitleBarAnimFrom1">#800080</Color>
    <Color x:Key="TitleBarAnimTo1">#0050FF</Color>
    <Color x:Key="TitleBarAnimFrom2">#C0502F</Color>
    <Color x:Key="TitleBarAnimTo2">#10A0D0</Color>
    
    <Color x:Key="CommIconColorFrom">#505050</Color>
    <Color x:Key="CommIconColorTo">#FF7FD4</Color>

    <Color x:Key="CoffButtonGColor">#FF7FD4</Color>

    <!-- SolidColorBrush 変換 -->
    <SolidColorBrush x:Key="LeftWindowColor1Brush" Color="{DynamicResource LeftWindowColor1}"/>
    <SolidColorBrush x:Key="LeftWindowColor2Brush" Color="{DynamicResource LeftWindowColor2}"/>
    <SolidColorBrush x:Key="CheckToggleColor1Brush" Color="{DynamicResource CheckToggleColor1}"/>
    <SolidColorBrush x:Key="CheckToggleColor2Brush" Color="{DynamicResource CheckToggleColor2}"/>
    <SolidColorBrush x:Key="AccentBrush" Color="{DynamicResource AccentColor}"/>
    <SolidColorBrush x:Key="ButtonThemeColorBrush" Color="{StaticResource ButtonThemeColor}"/>
    <SolidColorBrush x:Key="ButtonThemeColorBrush2" Color="{StaticResource ButtonThemeColor2}"/>
    <SolidColorBrush x:Key="BlacketColorBrush" Color="{DynamicResource BracketColor}"/>
    <SolidColorBrush x:Key="TextColorBrush" Color="{DynamicResource TextColor}"/>
    <SolidColorBrush x:Key="TagColorBrush" Color="{DynamicResource TagColor}"/>
    <SolidColorBrush x:Key="RippleColorBrush" Color="{DynamicResource RippleColor}"/>

    <!-- アイコンテンプレート定義 -->
    <x:Static x:Key="ThemeIconKind" Member="Icon:PackIconMynaUIIconsKind.MoonSolid"/>
</ResourceDictionary>